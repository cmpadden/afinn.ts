{
  "name": "afinn",
  "version": "0.0.1",
  "description": "AFINN sentiment analysis ported to Typescript",
  "repository": "https://github.com/cmpadden/afinn",
  "author": "Colton Padden",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "main": "./dist/afinn.umd.js",
  "module": "./dist/afinn.es.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/afinn.umd.js",
      "import": "./dist/afinn.es.js"
    }
  },
  "scripts": {
    "build": "vite build && tsc -p tsconfig.json --emitDeclarationOnly",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "lint": "eslint src/ --ext .js,.jsx,.ts,.tsx"
  },
  "devDependencies": {
    "@types/jest": "^27.4.0",
    "@types/node": "^17.0.20",
    "@typescript-eslint/eslint-plugin": "^5.9.1",
    "@typescript-eslint/parser": "^5.9.1",
    "c8": "^7.11.0",
    "esbuild": "^0.14.11",
    "eslint": "^8.6.0",
    "typescript": "^4.5.4",
    "vite": "^2.8.4",
    "vitest": "^0.5.0"
  }
}
